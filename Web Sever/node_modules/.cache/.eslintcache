,"38"],"/Users/oyls/react/test/src/App.js",["39","40","41","42","43","44","45"],"import React, {useState} from 'react';\nimport moment from 'moment';\n\nfunction App() {\n  const [nameUser, setName] = useState('');\n  const [age, setAge] = useState('');\n  const [date, setDate] = useState('');\n  const [phone, setPhone] = useState('');\n  const [email, setEmail] = useState('');\n  const [errorMessageName, setErrorMessageName] = useState('');\n  const [errorMessageAge, setErrorMessageAge] = useState('');\n  const [errorMessageDate, setErrorMessageDate] = useState('');\n  const [errorMessagePhone, setErrorMessagePhone] = useState('');\n  const [errorMessageEmail, setErrorMessageEmail] = useState('');\n  \n\n  function handleSubmit(e) {\n    e.preventDefault();\n\n      // NAME\n    const regName = /^[a-zA-Z]+\\s+[a-zA-Z]+$/;\n    const checkName = regName.test(nameUser);\n    if (!checkName) {\n      setErrorMessageName('*กรอกชื่อ นามสกุลให้ถูกต้อง');\n    } else {\n      setErrorMessageName('');\n    }\n    if (nameUser == 0) {\n      setErrorMessageName('*กรอกชื่อ นามสกุล');\n    }\n    \n    \n      // AGE\n    const regAge = /^[0-9]+$/;\n    const checkAge = regAge.test(age);\n    if (!checkAge) {\n      setErrorMessageAge('*กรอกเป็นตัวเลข')\n    } else {\n      setErrorMessageAge('')\n    }\n    if (age > 50) {\n      setErrorMessageAge('*อายุเกินเกณฑ์')\n    }\n    else if (age < 10 && age > 0) {\n      setErrorMessageAge('*อายุต่ำกว่าเกณฑ์')\n    }\n    else if (age == 0) {\n      setErrorMessageAge('*กรอกอายุ')\n    }\n\n      // DATE\n    const epochtime = Date.now();\n    const OneYear = 31556926;\n    const unixTimestamp = moment(date, 'YYYY-MM-DD').unix();\n    const getEpochtime = (epochtime/1000) - unixTimestamp;\n    const getYear = getEpochtime /  OneYear;\n    const years = Math.trunc(getYear);\n\n    if (years != age) {\n      setErrorMessageDate('*วัน-เดือน-ปี ไม่สัมพันธ์กับอายุ');\n    }\n    else if (getYear == 0) {\n      setErrorMessageDate('*กรอกวัน-เดือน-ปี')\n    }\n    else {\n      setErrorMessageDate('')\n    }\n    \n    // PHONE\n    const regPhone = /^[0-9]{3}-[0-9]{7}/;\n    const checkPhone = regPhone.test(phone);\n    if (!checkPhone) {\n        setErrorMessagePhone('*กรอก 0xx-xxxxxxx');\n      }else{\n        setErrorMessagePhone('');\n      }\n      if (phone == 0) {\n        setErrorMessagePhone('*กรอกเบอร์โทรศัพท์');\n      }\n\n\n      // EMAIL\n    const regMail = /^[a-zA-z0-9]+@[a-z]+.[a-z]+.[a-z]+.[a-z]+$/;\n    const checkMail = regMail.test(email);\n    if (!checkMail) {\n      setErrorMessageEmail('*กรอก email@xxx.xxx');\n    }else{\n        setErrorMessageEmail('');\n    }\n    if (email == 0) {\n      setErrorMessageEmail('*กรอกemail');\n    }\n\n      //SUBMIT\n    if (!checkName || !checkAge || !checkPhone || !checkMail || years != age) {\n      alert('กรอกข้อมูลไม่ถูกต้อง');\n    }\n    else {\n      alert('ลงทะเบียนเรียบร้อย');\n    }\n  }\n\n  return (\n    <div className=\"App\">\n      <form onSubmit={handleSubmit}>\n        <fieldset>\n          <legend>สมัครสมาชิก</legend>\n          <div className=\"name\">\n            <label>ชื่อ-นามสกุล : </label>\n            <input type=\"text\" placeholder=\"กรุณากรอกชื่อ นามสกุล เป็นภาษาอังกฤษ\"\n            onChange={e => setName(e.target.value)}></input>\n            {errorMessageName && <div className=\"error\">{errorMessageName}</div>}\n          </div>\n          <div className=\"gender\">\n            <label>เพศ : </label>\n            <select className=\"gender\">\n              <option value=\"\">เลือกเพศ</option>\n              <option value=\"\">หญิง</option>\n              <option value=\"\">ชาย</option>\n              <option value=\"\">อื่นๆ</option>\n            </select>\n          </div>\n          <div className=\"age\">\n            <label>อายุ : </label>\n            <input type=\"text\" maxLength=\"2\" onChange={e => setAge(e.target.value)}></input>\n            {errorMessageAge && <div className=\"error\"> {errorMessageAge}</div>}\n          </div>\n          <div className=\"date\">\n            <label>วัน/เดือน/ปี : </label>\n            <input type=\"date\" onChange={e => setDate(e.target.value)}></input>\n            {errorMessageDate && <div className=\"error\">{errorMessageDate}</div>}\n          </div>\n          <div className=\"phone\">\n            <label>เบอร์โทรศัพท์ : </label>\n            <input type=\"tel\" placeholder=\"0xx-xxxxxxx\" maxLength=\"11\"\n            onChange={e => setPhone(e.target.value)} ></input>\n            {errorMessagePhone && <div className=\"error\">{errorMessagePhone}</div>}\n          </div>\n          <div className=\"mail\">\n            <label>อีเมล์ : </label>\n            <input type=\"text\" placeholder=\"email@xxx.com\" \n            onChange={e => setEmail(e.target.value)}></input>\n            {errorMessageEmail && <div className=\"error\">{errorMessageEmail}</div>}\n          </div>\n        </fieldset>\n        <button>ลงทะเบียน</button>\n      </form>\n    </div>\n  );\n}\n\nexport default App;","/Users/oyls/register/Web Sever/src/index.js",[],"/Users/oyls/register/Web Sever/src/App.js",["46","47","48","49","50","51","52","53"],"/Users/oyls/Website-Validation/Web Sever/src/index.js",[],"/Users/oyls/Website-Validation/Web Sever/src/App.js",["54","55","56","57","58","59","60","61"],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"62","replacedBy":"66"},{"ruleId":"64","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":28,"column":18,"nodeType":"70","messageId":"71","endLine":28,"endColumn":20},{"ruleId":"68","severity":1,"message":"69","line":47,"column":18,"nodeType":"70","messageId":"71","endLine":47,"endColumn":20},{"ruleId":"68","severity":1,"message":"72","line":59,"column":15,"nodeType":"70","messageId":"71","endLine":59,"endColumn":17},{"ruleId":"68","severity":1,"message":"69","line":62,"column":22,"nodeType":"70","messageId":"71","endLine":62,"endColumn":24},{"ruleId